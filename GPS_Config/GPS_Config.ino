#include <SoftwareSerial.h>
SoftwareSerial gps(7, 8);

unsigned char baud[28] = {0xb5, 0x62, 0x06, 0x00, 0x14, 0x00, 0x01, 0x00, 0x00, 0x00, 0xd0, 0x08, 0x00, 0x00, 0x00, 0xc2, 0x01, 0x00, 0x07, 0x00, 0x07, 0x00, 0x00, 0x00, 0x00, 0x00, 0xc4, 0x96};
unsigned char GGA[16] = {0xB5,0x62,0x06,0x01,0x08,0x00,0xF0,0x00,0x00,0x00,0x00,0x00,0x00,0x01,0x00,0x24};
unsigned char GLL[16] = {0xB5,0x62,0x06,0x01,0x08,0x00,0xF0,0x01,0x00,0x00,0x00,0x00,0x00,0x01,0x01,0x2B};
unsigned char GSA[16] = {0xB5,0x62,0x06,0x01,0x08,0x00,0xF0,0x02,0x00,0x00,0x00,0x00,0x00,0x01,0x02,0x32};
unsigned char GSV[16] = {0xB5,0x62,0x06,0x01,0x08,0x00,0xF0,0x03,0x00,0x00,0x00,0x00,0x00,0x01,0x03,0x39};
unsigned char VTG[16] = {0xB5,0x62,0x06,0x01,0x08,0x00,0xF0,0x05,0x00,0x00,0x00,0x00,0x00,0x01,0x05,0x47};
unsigned char save[21] = {0xB5,0x62,0x06,0x09, 0x0D, 0x00, 0x00, 0x00, 0x00, 0x00, 0x03, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x17, 0x36, 0x86};

void setup() {
  pinMode(7, INPUT);
  pinMode(8, OUTPUT);
    
  Serial.begin(115200);
  gps.begin(9600);
  Serial.println("Start");

  delay(1000);

  for(int i=0; i < 28;i ++) {
    gps.write((byte)baud[i]);
  }
  gps.begin(115200);

  delay(1000);
  for(int i=0; i < 21;i ++) {
    gps.write((byte)save[i]);
  }
}

void loop() {
  if(gps.available()) {
    Serial.write(gps.read());
  }
}
